{
    "version": 2,
    "builds": [
      {
        "src": "api/*.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize":"15mb"}
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/main.py"
      }
    ]
  }